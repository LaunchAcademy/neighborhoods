Edit Users - Admin Page


<table>
  <% @members.each do |member| %>
  <tr>
    <td>
      <%= member.email %>
      <%= link_to 'Delete', admin_user_path(member), data: { confirm: "Are you sure?" }, method: :delete %>
      <%= link_to 'Promote', admin_user_path(member), data: { confirm: "Are you sure?" }, method: :patch %>
    </td>
  </tr>
  <% end %>
</table>
<h2>Admins</h2>
<table>
  <% @admins.each do |admin| %>
  <tr>
    <td>
      <%= admin.email %>
    </td>
  </tr>
  <% end %>
</table>
